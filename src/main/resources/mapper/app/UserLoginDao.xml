<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.micro.boot.app.dao.UserLoginDao">

	<!-- 可根据自己的需求，是否要使用，数据库字段映射 -->
	<resultMap type="com.micro.boot.app.object.request.UserLoginBean" id="userLoginMap">
		<result property="userId" column="user_id"/>
		<result property="username" column="username"/>
		<result property="mobile" column="mobile"/>
		<result property="password" column="password"/>
		<result property="createTime" column="create_time"/>
		<result property="token" column="token"/>
		<result property="expire" column="expire"/>
	</resultMap>


	<select id="queryByMobile2" resultType="com.micro.boot.app.object.request.UserLoginBean">
		select * from tb_user where mobile = #{value}
	</select>

</mapper>